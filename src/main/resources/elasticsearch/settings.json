{
	"index": {
		"analysis": {
			"tokenizer": {
				"ngrams_tokenizer": {
					"type": "ngram",
					"min_gram": 1,
					"max_gram": 5,
					"token_chars": [
						"letter", "digit", "punctuation", "symbol"
					]
				},
				"edge_ngrams_tokenizer_small": {
					"type": "edgeNGram",
					"min_gram": 1,
					"max_gram": 32,
					"token_chars": [
						"letter", "digit", "punctuation", "symbol"
					]
				}
			},
			"analyzer": {
				"ngrams": {
					"type": "custom",
					"tokenizer": "ngrams_tokenizer",
					"filter": ["lowercase"]
				},
				"edge_ngrams_small": {
					"type": "custom",
					"tokenizer": "edge_ngrams_tokenizer_small",
					"filter": ["lowercase"]
				}
			}
		}
	}
}
