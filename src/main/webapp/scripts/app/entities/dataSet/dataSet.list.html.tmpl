<div>
    <div  class="col-md-12">
      <div class="table-responsive">
        <table class="table table-hover">
      <thead>
      <tr>
        <th data-translate="metadatamanagementApp.dataSet.fdzId">FDZ ID</th>
        <th data-translate="metadatamanagementApp.dataSet.description.{{currentLanguage}}">title</th>
        <th data-translate="metadatamanagementApp.dataSet.surveyIds">surveyIds</th>
        <th data-translate="metadatamanagementApp.dataSet.variableIds">variableIds</th>
      </tr>
    </thead>
    <tbody>
        <tr ng-repeat="dataSet in currentPage._embedded.dataSets">
            <td>{{dataSet.id}}</td>
            <td>{{dataSet.description[currentLanguage]}}</td>
            <td><a ng-repeat="surveyId in dataSet.surveyIds" ui-sref="survey.detail({id:surveyId})">{{surveyId}}, </a></td>
            <td><a ng-repeat="variableId in dataSet.variableIds" ui-sref="variable.detail({id:variableId})">{{variableId}}, </a></td>
        </tr>
      </tbody>
    </table>
  </div>
  </div>
    <div  class="col-md-12 col-md-offset-4">
    <uib-pagination total-items="pageState.totalElements" ng-model="pageState.currentPageNumber" ng-change="pageChanged()" max-size="pageState.maxSize" class="pagination-sm" boundary-links="true" boundary-link-numbers="true" rotate="false" previous-text="{{'pager.previous' | translate}}" next-text="{{'pager.next' | translate}}" first-text="{{'pager.first' | translate}}" last-text="{{'pager.last' | translate}}"></uib-pagination>
  </div>
  <loading ng-if="!currentPage.$resolved"></loading>
</div>
