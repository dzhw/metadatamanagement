<md-card ui-sref="studyDetail({id: searchResult.id})" class="fdz-search-result">
    <md-card-header class="fdz-card-header-border">
        <md-card-avatar>
            <md-icon md-svg-src="/assets/images/icons/study.svg" class="fdz-search-result-icon"></md-icon>
        </md-card-avatar>
        <md-card-header-text class="fdz-truncate-string">
            <div class="md-title fdz-truncate-string">{{searchResult.title[currentLanguage]}} ({{searchResult.id}})</div>
            <div class="md-subhead fdz-truncate-string">{{searchResult.authors}}</div>
        </md-card-header-text>
    </md-card-header>
    <md-card-content>
        <div>{{searchResult.description[currentLanguage]}}</div>
    </md-card-content>
</md-card>
