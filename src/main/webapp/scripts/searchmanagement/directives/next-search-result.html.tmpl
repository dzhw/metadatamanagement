<md-button ng-if="url" class="md-primary md-fab md-mini fdz-next-search-result" ng-href="{{url}}" shortcut="ctrl-39">
    <md-tooltip md-direction="bottom" md-autohide="true" md-z-index="bowser.mobile || bowser.tablet ? -100 : 100">
        {{'search-management.buttons.next-search-result-tooltip' | translate : { id: nextSearchResult.id, index: nextSearchResultIndex } }}
    </md-tooltip>
    <md-icon md-font-set="material-icons">navigate_next</md-icon>
</md-button>
