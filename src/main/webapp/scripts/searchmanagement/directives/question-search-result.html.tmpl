<md-card ui-sref="questionDetail({id:searchResult.id})" class="fdz-search-result">
    <md-card-header class="fdz-card-header-border">
        <md-card-avatar>
            <md-icon md-svg-src="/assets/images/icons/question.svg" class="fdz-search-result-icon"></md-icon>
        </md-card-avatar>
        <md-card-header-text class="fdz-truncate-string">
            <div class="md-title fdz-truncate-string">{{'question-management.detail.question' | translate}} {{searchResult.number}} {{searchResult.surveyTitle}}</div>
            <div class="md-subhead fdz-truncate-string">{{searchResult.compositeQuestionName}}
                ({{searchResult.type}})</div>
        </md-card-header-text>
    </md-card-header>
    <md-card-content>
        <div>{{searchResult.number}}: {{searchResult.questionText}}</div>
    </md-card-content>
</md-card>
